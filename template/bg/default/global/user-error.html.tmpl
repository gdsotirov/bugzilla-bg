[%# INTERFACE:
  # header_done: boolean. True if the Bugzilla header has already been printed.
  # error: string. The tag of the error, or the error message to be displayed
  # (deprecated). May contain HTML if it's an error message.
  #%]

[%# This is a list of all the possible user errors. Please keep them in
  # alphabetical order by error tag, and leave a blank line between errors.
  #
  # Note that you must explicitly filter every single template variable
  # in this file; if you do not wish to change it, use the "none" filter.
  #
  # Extension- or custom-specific error handling  can be easily added 
  # via hooks: just place your <extension>-errors.html.tmpl into 
  # template/en/extension/hook/global/user-error.html.tmpl/errors/
  # Note: be aware of uniqueness of error string parameter value, since 
  # nobody can guarantee the hook files processing order in the future
  #%]

[% PROCESS global/variables.none.tmpl %]

[% PROCESS "global/field-descs.none.tmpl" %]

[% DEFAULT title = "Грешка" %]

[% error_message = BLOCK %]
  [% IF    error == "account_creation_disabled" %]
    [% title = "Създаването на потребители е невъзможно" %]
    Създаването на потребители е забранено или ограничено.
    <hr>
    Нови потребители трябва да бъдат създавани от администратор.

  [% ELSIF error == "account_disabled" %]
    [% title = "Извадена от употреба регистрация" %]
    [% disabled_reason FILTER none %]
    <hr>
    Ако вярвате, че регистрацията ви трябва да бъде възстановена, моля
    изпратете поща до [% Param("maintainer") %], обяснявайки защо.

  [% ELSIF error == "account_exists" %]
    [% title = "Регистрацията вече съществува" %]
    Вече има регистрация с
    [% IF email %]
      потребителското име [% email FILTER html %].
    [% ELSE %]
      това потребителско име.
    [% END %]

  [% ELSIF error == "account_inexistent" %]
    [% title = "Регистрацията не съществува" %]
    Няма [% terms.Bugzilla %] регистрация с това име.

  [% ELSIF error == "alias_has_comma_or_space" %]
    [% title = "Неправилни символи в псевдоним" %]
    Псевдонима, който сте въвели, <em>[% alias FILTER html %]</em>,
    съдържа една или повече запетайки или интервали. Псевдонимите не могат да
    съдържат запетайки или интервали, защото тези символи се използват за
    разделяне на псевдоними един от друг в списъци.  Моля изберете друг
    псевдоним, който не съдържа запетайки или интервали.

  [% ELSIF error == "alias_in_use" %]
    [% title = "Псевдонима се използва" %]
    [% terms.Bug %] [%+ bug_id FILTER bug_link(bug_id) FILTER none %]
    вече е взел псевдонима <em>[% alias FILTER html %]</em>.
    Моля, изберете друг.

  [% ELSIF error == "alias_not_defined" %]
    [% title = "Псевдонима не е зададен" %]
    Не сте предоставили псевдоним за този [% terms.bug %].

  [% ELSIF error == "alias_is_numeric" %]
    [% title = "Псевдонима е цифров" %]
    Опитахте се да зададете псевдонима <em>[% alias FILTER html %]</em> на
    този [% terms.bug %], но псевдонимите не могат да бъдат само числа,
    тъй като така могат да бъдат объркани с номер на [% terms.bug %].
    Моля, изберете друг псевдоним съдържащ поне една буква.

  [% ELSIF error == "alias_too_long" %]
    [% title = "Псевдонима е твърде дълъг" %]
    Псевдонимите на [% terms.bug %] не могат да бъдат по-дълги от 20 символа.
    Моля, изберете по-къс псевдоним.

  [% ELSIF error == "auth_cant_create_account" %]
    [% title = "Не може да се създават потребители" %]
    Този сайт използва схема на удостоверяване, която не разрешава създаването
    на потребители. Моля, свържете се с администратор, за да Ви бъде създаден
    потребител.

  [% ELSIF error == "auth_failure" %]
    [% title = "Необходимо е удостоверяване" %]
    [% admindocslinks = {'groups.html' => 'Групова сигурност'} %]
    Съжалявам,
    [% IF group %]
      не сте член на групата '[% group FILTER html %]',
    [% END %]

    [% IF reason %]
      [% IF group %] и [% END %]
      [% IF reason == "cant_bless" %]
        нямате права да добавяте или премахвате хора от група,
      [% ELSIF reason == "not_visible" %]
        има ограничения над видимостта върху определени групи,
      [% END %]
    [% END %]

    [% IF group || reason %], така че [% END %] не сте упълномощен да
    [% IF action == "access" %]
      достъпвате
    [% ELSIF action == "add" %]
      добавяте нов
    [% ELSIF action == "begin" %]
      започвате
    [% ELSIF action == "modify" %]
      изменяте
    [% ELSIF action == "delete" %]
      изтривате
    [% ELSIF action == "edit" %]
      добавяте, изменяте или изтривате
    [% ELSIF action == "move" %]
      премествате
    [% ELSIF action == "run" %]
      пускате
    [% ELSIF action == "schedule" %]
      разписвате
    [% ELSIF action == "use" %]
      използвате
    [% END %]

    [% IF object == "attachment" %]
      тази приставка
    [% ELSIF object == "bugs" %]
      [%+ terms.bugs %]
    [% ELSIF object == "charts" %]
      възможността "Нови графики"
    [% ELSIF object == "classifications" %]
      класификации
    [% ELSIF object == "components" %]
      съставни части
    [% ELSIF object == "custom_fields" %]
      потребителски полета
    [% ELSIF object == "field_values" %]
      стойности на полета
    [% ELSIF object == "flagtypes" %]
      типове флагове
    [% ELSIF object == "group_access" %]
      групов достъп
    [% ELSIF object == "groups" %]
      групи
    [% ELSIF object == "keywords" %]
      ключови думи
    [% ELSIF object == "milestones" %]
      важни събития
    [% ELSIF object == "multiple_bugs" %]
      няколко [% terms.abug %] едновременно
    [% ELSIF object == "parameters" %]
      параметри
    [% ELSIF object == "products" %]
      продукти
    [% ELSIF object == "quips" %]
      остроумия
    [% ELSIF object == "reports" %]
      whine доклади
    [% ELSIF object == "sanity_check" %]
      санитарна проверка
    [% ELSIF object == "settings" %]
      настройки
    [% ELSIF object == "sudo_session" %]
      сесия като друг потребител
    [% ELSIF object == "timetracking_summaries" %]
      обобщени доклади за следене на времетраенето
    [% ELSIF object == "user" %]
      указания от вас потребител
    [% ELSIF object == "users" %]
      потребители
    [% ELSIF object == "versions" %]
      версии
    [% END %].

    [% Hook.process("auth_failure") %]

  [% ELSIF error == "attachment_deletion_disabled" %]
    [% title = "Изтриването на приставки е забранено" %]
    Изтриването на приставки е забранено на тази инсталация.

  [% ELSIF error == "attachment_removed" %]
    [% title = "Приставката е премахната" %]
    Приставката, която се опитвате да достъпите беше премахната.

  [% ELSIF error == "bug_access_denied" %]
    [% title = "Достъпът отказан" %]
    [% admindocslinks = {'groups.html' => 'Групова сигурност'} %]
    Не ви е позволен достъп до [% terms.bug %] №[% bug_id FILTER html %].

  [% ELSIF error == "bug_access_query" %]
    [% title = "Достъпът отказан" %]
    [% docslinks = {'myaccount.html' => 'Създаване на сметка'} %]
    Не ви е позволен достъп до [% terms.bug %] №[% bug_id FILTER html %].
    За да видите този [% terms.bug %] първо
    трябва <a href="show_bug.cgi?id=
                   [% bug_id FILTER url_quote %]&amp;GoAheadAndLogIn=1">влезете
    като потребител</a> с подходящите позволения.

  [% ELSIF error == "buglist_parameters_required" %]
    [% title = "Необходими са параметри" %]
    [% docslinks = {'query.html' => "Търсене за $terms.bugs",
                    'query.html#list'  => "$terms.Bug lists"} %]
    Не можете да търсите или да създавате съхранени търсения без никакви термини за търсене.

  [% ELSIF error == "bugs_not_changed" %]
    [% title = BLOCK %][% terms.Bugs %]те не са променени[% END %]
    Изглежда не сте направили никакви промени по избраните [% terms.bugs %],
    така че няма нищо за предаване.

  [% ELSIF error == "chart_too_large" %]
    [% title = "Графиката твърде голяма" %]
    Съжалявам, но 2000 x 2000 пиксела е максималният размер за графика.

  [% ELSIF error == "comment_required" %]
    [% title = "Необходим е коментар" %]
    Трябва да попълните <b>коментар</b> за тази промяна.
    Моля, обяснете я.

  [% ELSIF error == "comment_too_long" %]
    [% title = "Коментарът твърде дълъг" %]
    Коментарите не могат да бъдат по-дълги от 65,535 символа.

  [% ELSIF error == "auth_classification_not_enabled" %]
    [% title = "Класификацията не е позволена" %]
    Съжалявам, класофикацията не е позволена.

  [% ELSIF error == "classification_not_specified" %]
    [% title = "Трябва да предоставите име на класификацията" %]
    Трябва да въведете име на класификация.

  [% ELSIF error == "classification_already_exists" %]
    [% title = "Класификацията вече съществува" %]
    Класификация с име '[% name FILTER html %]' вече съществува.

  [% ELSIF error == "classification_doesnt_exist" %]
    [% title = "Класификацията не съществува" %]
    Класификацията '[% name FILTER html %]' не съществува.

  [% ELSIF error == "classification_doesnt_exist_for_product" %]
    [% title = "Класификацията не съществува за продукт" %]
    Класификацията '[% classification FILTER html %]' не съществува
    за продукта '[% product FILTER html %]'.

  [% ELSIF error == "classification_invalid_sortkey" %]
    [% title = "Недействителен ключ за подреждане на класификация" %]
    Ключа за пдреждане <em>[% sortkey FILTER html %]</em> за класификацията '[% name FILTER html %]'
    е недействителен. Трябва да бъде положително цяло цисло.

  [% ELSIF error == "classification_not_deletable" %]
    [% title = "Подразбиращата се класификация не може да бъде изтрита" %]
    Не можете да изтриете подразбиращата се класификация

  [% ELSIF error == "classification_has_products" %]
     Съжалявам, има продукти за тази класификация. Трябва
     да преназначите тези продукти на друга класификация преди
     да можете да изтриете тази.

  [% ELSIF error == "component_already_exists" %]
    [% title = "Съставната част вече съществува" %]
    Съставна част с името '[% name FILTER html %]' вече съществува.

  [% ELSIF error == "component_blank_description" %]
    [% title = "Не е позволено празно описание на съставна част" %]
    Трябва да въведете някакво описание за съставната част '[% name FILTER html %]'.

  [% ELSIF error == "component_blank_name" %]
    [% title = "Не е позволено празно име на съставна част" %]
    Трябва да въведете име за новата съставна част.

  [% ELSIF error == "component_has_bugs" %]
    [% title = BLOCK %]Съставната част има [% terms.bugs %][% END %]
    Съществуват [% nb FILTER html %] [%+ terms.bug %](а) въведени за съставната част!
    Трябва да преназначите тези [% terms.bugs %] на друга съставна част, преди да
    можете да изтриете тази.

  [% ELSIF error == "component_name_too_long" %]
    [% title = "Името на съставната част е твърде дълго" %]
    Името на съставна част е ограничено до 64 символа. 
    '[% name FILTER html %]' е твърде дълго ([% name.size %] символа).

  [% ELSIF error == "component_need_initialowner" %]
    [% title = "Съставната част изисква подразбиращо се назначение" %]
    Трябва да въведете подразбиращо се назначение за съставната част '[% name FILTER html %]'.

  [% ELSIF error == "component_not_valid" %]
    [% title = "Указаната съставна част не съществува" %]
    Продукта '[% product FILTER html %]' няма съставна част
    на име '[% name FILTER html %]'.

  [% ELSIF error == "customfield_nonexistent" %]
    [% title = "Непознато потребителско поле" %]
    Няма потребителско поле с името '[% name FILTER html %]'.

  [% ELSIF error == "dependency_loop_multi" %]
    [% title = "Установена циклична зависимост" %]
    Следващия(те) [% terms.bug %](ове) ще се появят и на "зависи от"
    и на "блокира" частите на дървото на зависимостите ако тези промени
    бъдат предадени:
    [% FOREACH dep = deps %]
      [%+ dep FILTER bug_link(dep) FILTER none %]
    [% END %].
    Това би създало циклична зависимост, което не е позволено.

  [% ELSIF error == "dependency_loop_single" %]
    [% title = "Установено зацикляне на зависимост" %]
    Не можете да направите [% terms.abug %] да блокира себе си или да зависи от самия себе си.

  [% ELSIF error == "description_required" %]
    [% title = "Необходимо е описание" %]
    Трябва да осигурите описание за [% terms.abug %].

  [% ELSIF error == "dupe_not_allowed" %]
    [% title = "Невъзможност за отбелязване на $terms.bugs като двойници" %]
    Не можете да отбелязвате [% terms.bugs %] като двойници когато
    променяте няколко [% terms.abug %] наведнъж.

  [% ELSIF error == "dupe_loop_detected" %]
    [% title = "Установено зацикляне между двойници" %]
    Не можете да отбележите [% terms.bug %] [%+ bug_id FILTER html %] като
    двойник на
    [% IF dupe_of == bug_id %]
      себе си
    [% ELSE %]
      [%+ terms.bug %] [%+ dupe_of FILTER html %], защото това
      ще създаде циклична зависимост
    [% END %].

  [% ELSIF error == "email_change_in_progress" %]
    [% title = "Промяната на е-пощенски адрес се обработва" %]
    Промяната на е-пощенски адрес се обработва; моля, проверете пощата си.

  [% ELSIF error == "email_confirmation_failed" %]
    [% title = "Потвърждението на е-пощенски адрес пропадна" %]
    Потвърждението на е-пощенски адрес пропадна.

  [% ELSIF error == "email_no_text_plain" %]
    Съобщението ви не съдържа никакъв текст. [% terms.Bugzilla %] не
    приема само HTML е-поща, или HTML е-поща с приставки.

  [% ELSIF error == "empty_group_description" %]
    [% title = "Описанието на група не може да е празно" %]
    Трябва да въведете описание за групата.

  [% ELSIF error == "empty_group_name" %]
    [% title = "Името на група на може да е празно" %]
    Трябва да въведете име за групата.

  [% ELSIF error == "entry_access_denied" %]
    [% title = "Достъпът отказан" %]
[% admindocslinks = {'groups.html' => 'Групова сигурност'} %]
    Съжалявам, или продукта <em>[% product FILTER html %]</em>
    не съществува или не сте упълномощен да
    въвеждате [% terms.bug %] за него.

  [% ELSIF error == "field_already_exists" %]
    [% title = "Полето вече съществува" %]
    Полето '[% field.name FILTER html %]'     
    ([% field.description FILTER html %]) вече съществува. Моля,
    изберете друго име.

  [% ELSIF error == "field_invalid_name" %]
    [% title = "Недействително име на поле" %]
    '[% name FILTER html %]' не е действително име за поле.
    Името може да съдържа само букви, цифри и знака за подчертване.

  [% ELSIF error == "field_invalid_sortkey" %]
    [% title = "Недействителен ключ за подреждане на поле" %]
    Ключа за подреждане [% sortkey FILTER html %], който сте предоставили за
    полето в действителност не е положително число.

  [% ELSIF error == "field_missing_description" %]
    [% title = "Липса описание на поле" %]
    Трябва да въведете описание за това поле.

  [% ELSIF error == "field_missing_name" %]
    [% title = "Липсва име на поле" %]
    Трябва да въведете име за това поле.

  [% ELSIF error == "fieldname_invalid" %]
    [% title = "Указаното поле не съществува" %]
    Полето '[% field FILTER html %]' не съществува или 
    не може да бъде редактирано през този интерфейс.

  [% ELSIF error == "fieldname_not_specified" %]
    [% title = "Не е указано име за полето" %]
    Не е указано име на полето при опит за редакция на стойностите му.

  [% ELSIF error == "fieldvalue_already_exists" %]
    [% title = "Стойността вече съществува за полето" %]
    Стойността '[% value FILTER html %]' вече съществува за полето
    '[%- field FILTER html %]'.

  [% ELSIF error == "fieldvalue_doesnt_exist" %]
    [% title = "Указаната стойност на поле не съществува" %]
    Стойността '[% value FILTER html %]' не съществува за
    полето '[% field FILTER html %]'.

  [% ELSIF error == "fieldvalue_is_default" %]
    [% title = "Указаната стойност е подразбираща се" %]
    '[% value FILTER html %]' е стойност по подразбиране за
    полето '[% field FILTER html %]' и не може да бъде изтрита.
    [% IF user.groups.tweakparams %]
      Трябва първо да <a href="editparams.cgi?section=bugfields#
      [%- param_name FILTER url_quote %]">промените</a> стойността по подразбиране.
    [% END %]

  [% ELSIF error == "fieldvalue_name_too_long" %]
    [% title = "Стойността на полето е твърде дълга" %]
    Стойността на поле е ограничена до 60 символа.
    '[% value FILTER html %]' е твърде дълга ([% value.length %] символа).

  [% ELSIF error == "fieldvalue_not_editable" %]
    [% title = "Стойност на поле не може да се променя" %]
    Стойността '[% old_value FILTER html %]' не може да бъде преименуване, защото
    тя играе някаква специална роля за полето '[% field FILTER html %]'.

  [% ELSIF error == "fieldvalue_not_deletable" %]
    [% title = "Стойност не поле е неизтриваема" %]
    Стойността '[% value FILTER html %]' не може да бъде премахната, защото
    тя играе някаква специална роля за полето '[% field FILTER html %]'.

  [% ELSIF error == "fieldvalue_not_specified" %]
    [% title = "Не е указана стойност на поле" %]
    Не е указана стойност на поле при опита за редакцията и.

  [% ELSIF error == "fieldvalue_sortkey_invalid" %]
