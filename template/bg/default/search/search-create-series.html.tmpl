[%# INTERFACE:
  # This template has no interface. However, to use it, you need to fulfill
  # the interfaces of search/form.html.tmpl, reports/series.html.tmpl and
  # search/boolean-charts.html.tmpl.
  #%]

[% js_data = BLOCK %]
    var queryform = "chartform";
[% END %]

[% PROCESS global/header.html.tmpl 
  title = "Създаване на нов набор данни"
  onload = "doOnSelectProduct(0);"
  yui = [ 'autocomplete', 'calendar' ]
  javascript = js_data 
  javascript_urls = [ "js/util.js", "js/TUI.js", "js/field.js" ]
  style_urls = [ "skins/standard/search_form.css" ]
  doc_section = "reporting.html#charts-new-series"
%]

<form method="get" action="chart.cgi" name="chartform">
  
[% PROCESS search/form.html.tmpl %]

<p>
  <input type="submit" id="action-search" name="action-search" value="Пускане на търсене">,
  за да се види кои [% terms.bugs %] ще бъдат включени в набора данни.
</p>
    
<h3>Параметри на набор от данни</h3>
      
[% PROCESS reports/series.html.tmpl 
   button_name = "Създаване на набор данни" %]
  <input type="hidden" name="action" value="create">
  <input type="hidden" name="token" value="[% issue_hash_token(['create-series']) FILTER html %]">

<script type="text/javascript">
  document.chartform.category[0].selected = true;
  catSelected();
  checkNewState();
</script>
      
<hr>

[% PROCESS "search/boolean-charts.html.tmpl" %]

</form>

[% PROCESS global/footer.html.tmpl %]
