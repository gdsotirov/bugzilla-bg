[%# INTERFACE:
  # name: string; the name of the deleted milestone.
  #
  # product: string; the name of the product the milestone belonged to
  #
  # bug_count: number; the number of bugs which were reassigned to
  #            the default milestone.
  #%]
  
[% title = BLOCK %]Събитието '[% name FILTER html %]' на продукта
                   '[% product FILTER html %]' бе изтрито[% END %]
[% PROCESS global/header.html.tmpl
  title = title
%]

<p>
[% IF bug_count %]

  [% bug_count %]
  [% IF bug_count > 1 %] 
    [%+ terms.abug %] преназначени
  [% ELSE %]
    [%+ terms.bug %] преназначен
  [% END %]
  на подразбиращото се събитие.

[% ELSE %]
  Няма [% terms.bugs %], които целяха събитието.
[% END %]
</p>

<p>Събитието '[% name FILTER html %]' бе изтрито.</p>

<p>

[% PROCESS admin/milestones/footer.html.tmpl
  no_edit_milestone_link = 1
 %]

[% PROCESS global/footer.html.tmpl %]
